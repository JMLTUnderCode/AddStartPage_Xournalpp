<?xml version="1.0" encoding="UTF-8"?>
<interface>
	<requires lib="gtk+" version="3.20"/>
	<object class="GtkApplicationWindow" id="windowhome">
		<property name="name">windowhome</property>
		<property name="title">Home Page</property>
		<property name="default_width">800</property>
		<property name="default_height">600</property>
		<child>
			<object class="GtkBox" id="box1">
				<property name="name">box1</property>
				<property name="visible">True</property>
				<property name="can_focus">False</property>
				<property name="orientation">GTK_ORIENTATION_VERTICAL</property>
				<child>
					<object class="GtkLabel" id="label1">
						<property name="name">label1</property>
						<property name="visible">True</property>
						<property name="can_focus">False</property>
						<property name="label" translatable="yes">Welcome to the Home Page</property>
					</object>
				</child>
				<child>
					<object class="GtkBox" id="newDocument_button_box">
						<property name="visible">True</property>
						<property name="can_focus">False</property>
						<property name="orientation">horizontal</property>
						<property name="halign">center</property>
						<child>
							<object class="GtkButton" id="newDocument_button">
								<property name="label" translatable="yes">New Document</property>
								<property name="width_request">40</property>
								<property name="height_request">30</property>
								<property name="visible">True</property>
								<property name="can_focus">True</property>
								<property name="can_default">True</property>
								<property name="has_default">True</property>
								<property name="receives_default">True</property>
								<property name="margin-top">50</property>
								<accelerator key="Enter" signal="clicked"/>
							</object>
						</child>
					</object>
				</child>
				<child>
					<object class="GtkBox" id="openRecentDocument_button_box">
						<property name="visible">True</property>
						<property name="can_focus">False</property>
						<property name="orientation">horizontal</property>
						<property name="halign">center</property>
						<child>
							<object class="GtkButton" id="openRecentDocument_button">
								<property name="label" translatable="yes">Open Recent Document</property>
								<property name="width_request">40</property>
								<property name="height_request">30</property>
								<property name="visible">True</property>
								<property name="can_focus">True</property>
								<property name="can_default">True</property>
								<property name="has_default">True</property>
								<property name="receives_default">True</property>
								<property name="margin-top">50</property>
								<accelerator key="Escape" signal="clicked"/>
							</object>
						</child>
					</object>
				</child>
			</object>
		</child>
	</object>
</interface>